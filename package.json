{
    "name": "@zeitport/eslint-parser-json",
    "version": "1.0.0",
    "description": "A JSON parser for ESLint.",
    "main": "index.js",
    "scripts": {
        "lint": "eslint lib",
        "test": "ava --verbose ./test/lib/*.js ./test/rules/*.js",
        "preversion": "npm test",
        "postversion": "git push && git push --tags",
        "postpublish": "npm version prepatch && git push && git push --tags"
    },
    "files": [
        "README.md",
        "src/**/*.js",
        "index.js",
        "LICENSE",
        "config"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zeitport/eslint-parser-json.git"
    },
    "keywords": [
        "eslint",
        "eslint-parser",
        "JSON",
        "parser"
    ],
    "author": "Christian Schuller <cschuller@servusalps.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zeitport/eslint-parser-json/issues"
    },
    "homepage": "https://github.com/zeitport/eslint-parser-json#readme",
    "dependencies": {
        "acorn": "^7.4.0",
        "eslint-visitor-keys": "^1.3.0",
        "espree": "^7.3.0"
    },
    "peerDependencies": {
        "eslint": ">= 7"
    },
    "devDependencies": {
        "ava": "^3.12.1",
        "eslint": "^7.8.1"
    }
}
